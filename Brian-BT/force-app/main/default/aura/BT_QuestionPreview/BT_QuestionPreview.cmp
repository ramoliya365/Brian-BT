<aura:component controller="BT_QuestionnaireBuilder" implements="force:appHostable,flexipage:availableForRecordHome,flexipage:availableForAllPageTypes,lightning:actionOverride,force:hasRecordId,forceCommunity:availableForAllPageTypes" access="global">
    <aura:attribute name="selectionId" type="String" />
    <aura:attribute name="imageVisibility" type="Boolean" default="false" />
    <aura:attribute name="questionGroupId" type="String" access="public" description="Question group Id" />
    <aura:attribute name="isOptionSaved" type="Boolean" access="public" default="false" />
    <aura:attribute name="questionGroup" type="object" access="public" description="Question group details" />

    <!-- list which contains the id of files for option -->
    <aura:attribute name="optionDocList" type="List" access="public" />

    <aura:attribute name="questions" type="List" access="public" description="Question details" />
    <aura:attribute name="questionsCopy" type="List" access="public" description="Question details" />
    <aura:attribute name="autoload" type="Boolean" access="public" description="auto load quetion from question group" default="true" />
    <aura:attribute name="isLocked" type="Boolean" access="public" description="auto load quetion from question group" default="false" />
    <aura:attribute name="isOverageTurnedOff" type="Boolean" access="public" default="false" />
    <aura:attribute name="isCustomerCommunity" type="Boolean" access="public" default="false" />
    <aura:attribute name="prefixURL" type="String" default="/sfc/servlet.shepherd/version/download/" />
    <aura:attribute name="questionLoaded" type="Boolean" access="public" description="This boolean will be true once all question list loaded through aura itration." />
    <aura:attribute name="callerName" type="String" />
    <aura:attribute name="readOnly" type="Boolean" default="false" />
    <aura:attribute name="filetype" type="List" default="['.png', '.jpg', '.jpeg']" />
    <aura:attribute name="multiple" type="Boolean" default="true" />
    <aura:attribute name="isOptionImageDelete" type="Boolean" default="false" />
    <aura:attribute name="optionImageDeleteId" type="String" />
    <aura:attribute name="disabled" type="Boolean" default="true" />
    <aura:attribute name="requiredReload" type="Boolean" default="false" />
    <aura:attribute name="currentDocumentId" type="String" default="" />
    <aura:attribute name="hasModalOpen" type="String" default="false" />
    <aura:attribute name="currentIndex" type="String" default="false" />
    <aura:attribute name="modalPromise" type="Aura.Action" />
    <aura:attribute name="isResponse" type="Boolean" default="false" />
    <aura:attribute name="totalOptions" type="Integer" default="0" />
    <aura:attribute name="errorMessage" type="String" default="" />
    <aura:attribute name="projectName" type="string" />
    <aura:attribute name="optionId" type="string" />
    <aura:attribute name="isChangeOrder" type="Boolean" default="false" />
    <aura:attribute name="isCreateChangeOrderonOverage" type="Boolean" default="false" />
    <aura:attribute name="overlayLib" type="String" description="Used to show force data service error" />
    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    <aura:registerevent name="componentEventFired" type="c:errorMessageEvent" />
    <aura:handler event="c:BT_TreeNodeClickEvent" action="{!c.handleTreeNodeClick}" />




    <div style="{!or(v.questions.length==0,not(v.isResponse))?'display:none;':'color:#d4504c;width:100%;padding: 20px 0px;'}">
        <div style="width: 100%;white-space: normal">
            <div style="text-align:right;float:right;">
                <lightning:button variant="brand" class="{!v.isLocked?'slds-hide':'slds-show marginBottom'}" label="Save" title="Save" onclick="{! c.savaeRecordModal}" />
                <lightning:icon iconName="utility:lock" class="{!v.isLocked?'slds-show':'slds-hide'}" alternativeText="locked" size="small" />
            </div>
        </div>
    </div>

    <table class="{!or(v.questions == null,v.questions.length==0)?'slds-hide':'slds-table slds-table_bordered slds-box'}" style="padding:0px;table-layout:fixed;">
        <thead>
            <tr class="slds-line-height_reset" style='width: 15%;white-space: normal;'>
                <aura:if isTrue="{!and(!v.isLocked,v.isResponse)}">
                    <th style="width:3% !important;">
                        <div title="checkBox"></div>
                    </th>
                </aura:if>
                <aura:if isTrue="{!and(v.isLocked,v.isResponse)}">
                    <th style="width:3% !important;">
                        <div title="checkBox"></div>
                    </th>
                </aura:if>

                <aura:if isTrue="{!!v.isResponse}">
                    <th style="width:3% !important;">
                        <div title="checkBox"></div>
                    </th>
                </aura:if>

                <aura:if isTrue="{!!v.imageVisibility}">
                    <th class="" scope="col">
                        <div class="slds-truncate" title="Option Image"></div>
                    </th>
                </aura:if>

                <th style="width:15% !important;">
                    <div title="Option Name">Option Name</div>
                </th>

                <th style="width:15% !important;">
                    <div title="Details">Details</div>
                </th>

                <th style="width:12% !important;">
                    <div title="Attributes">Attributes</div>
                </th>
                <th style="width:35% !important;">
                    <div title="Child Attributes"></div>
                </th>

                <th style="width:3% !important;">
                    <div title="Actions"></div>
                </th>
            </tr>
        </thead>
        <tbody>
            <aura:iteration items="{!v.questions}" var="question" loaded="{!v.questionLoaded}" indexVar="index">
                <aura:if isTrue="{!and(v.isLocked,question.check.buildertek__Is_Selected_Item__c)}">
                    <tr style="{!if(and(question.errorMessage != null,question.errorMessage != ''),'color:#d4504c;width:80%;padding: 20px 0px;','width:80%;padding: 20px 0px;')}" class="highlight" id="{!question.check.Id}">
                        <td style="{!v.isResponse?' width: 3%;white-space: normal':'display:none;'}">
                            <div class="{!v.isLocked?'slds-hide':''}">
                                <lightning:input type="checkbox" variant="lable-hidden" disabled="{!or(not(v.isResponse),question.check.buildertek__Is_Selected_Item__c)}" checked="{!question.check.buildertek__Is_Selected_Item__c}" name="inputCheckbox" />
                            </div>
                        </td>

                        <!--image -->
                        <aura:if isTrue="{!!v.imageVisibility}">
                            <td data-label="Option Image" scope="row">
                                <div class="slds-truncate">
                                    <td style="{!v.isResponse?' width: 25%;white-space: normal':' width: 22%;white-space: normal'}">
                                        <div class="{!'fileUpload divFileUpload_' + question.check.Id}">
                                            <aura:if isTrue="{!not(v.isResponse)}">
                                                <span style="width:98%;float:left;">
                                            <lightning:fileUpload label="Attach receipt" name="fileUploader"
                                                multiple="true" accept="{!v.filetype}" recordId="{!question.check.Id}"
                                                onuploadfinished="{!c.handleUploadFinished}"
                                                class="{!'fileUpload_' + question.check.Id}" />
                                        </span>
                                            </aura:if>

                                            <span data-id="{!index}" data-checkId="{!question.check.Id}" onclick="{!c.onCancelReuploadPhoto}" style="float:right;">
                                        <lightning:icon iconName="utility:close" size="xx-small" title="cancel" />
                                    </span>
                                        </div>
                                        <div class="{!'imageFileUploadCon_' + question.check.Id}">
                                            <aura:if isTrue="{!not(empty(question.check.buildertek__Temp_Image_Id__c))}">
                                                <aura:if isTrue="{!v.isResponse}">
                                                    <div style="float:right;margin:5px;display:inline;">
                                                        <span data-id="{!index}" data-checkId="{!question.check.Id}" onclick="{!c.onReuploadPhoto}">
                                                    <lightning:icon iconName="utility:edit" size="xx-small"
                                                        title="Edit" />
                                                </span>
                                                        <span data-id="{!index}" data-checkId="{!question.check.Id}" onclick="{!c.deleteOptionImage}">
                                                    <lightning:icon iconName="utility:delete" size="xx-small"
                                                        title="Delete" />
                                                </span>
                                                    </div>
                                                </aura:if>
                                                <aura:set attribute="else">
                                                    <span data-id="{!index}" data-checkId="{!question.check.Id}" onclick="{!c.onReuploadPhoto}" style="{!v.isResponse?'display:none;':'float:right;'}">
                                                <lightning:icon iconName="utility:edit" size="xx-small" title="Edit" />
                                            </span>
                                                    <img src="{!$Resource.NoImageFound}" width="150" height="150" data-id="{!index}" data-checkId="{!question.check.Id}" />
                                                </aura:set>
                                                <aura:if isTrue="{!v.isCustomerCommunity}">
                                                    <img src="{!'/Customer/sfc/servlet.shepherd/version/download/'+question.check.buildertek__Temp_Image_Id__c}" width="150" height="150" data-id="{!index}" onclick="{!c.onPhotoShow}" data-checkId="{!question.check.Id}" />
                                                    <aura:set attribute="else">
                                                        <img src="{!'/sfc/servlet.shepherd/version/download/'+question.check.buildertek__Temp_Image_Id__c}" width="150" height="150" data-id="{!index}" onclick="{!c.onPhotoShow}" data-checkId="{!question.check.Id}" />
                                                    </aura:set>
                                                </aura:if>
                                            </aura:if>
                                        </div>
                                    </td>
                                </div>
                            </td>
                        </aura:if>

                        <td data-label="Option Name" scope="row">
                            <div class="slds-truncate">
                                <div style="float:left;">
                                    <aura:unescapedHtml value="{!question.check.Name}" />
                                </div> &nbsp;
                            </div>
                        </td>

                        <!-- Question text with help text -->
                        <td data-label="Details" style="width: 19%;white-space: normal">
                            <div>
                                <p>Additional Cost:
                                    <ui:outputCurrency value="{!question.check.buildertek__Additional_Cost__c!=undefined?question.check.buildertek__Additional_Cost__c:0}" />
                                </p>
                            </div>
                        </td>

                        <td data-label="Attributes" style="width: 20%;white-space: normal">
                            <aura:if isTrue="{!and(question.errorMessage != null,question.errorMessage != '')}">
                                <div style="float:left;margin-top: -6px;">
                                    <lightning:icon iconName="utility:warning" alternativeText="Error!" variant="error" title="{!question.errorMessage}" /> &nbsp;
                                </div>
                            </aura:if>

                            <aura:iteration items="{!question.responses}" var="itemArr" indexVar="attributeIndex">
                                <!--  Simple Text  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Text~Simple Text',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <div class="">
                                        <ui:outputText value="{!itemArr.Name}" />
                                        <lightning:input disabled="{!v.readOnly}" variant="label-hidden" class="" required="{!question.check.buildertek__Mandatory__c}" value="{!itemArr.values}" />
                                    </div>
                                </aura:if>

                                <!--  Text Area  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Text~Text Area',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <div class="">
                                        <ui:outputText value="{!itemArr.Name}" />
                                        <lightning:textarea disabled="{!v.readOnly}" variant="label-hidden" class="" required="{!question.check.buildertek__Mandatory__c}" value="{!itemArr.values}" />
                                    </div>
                                </aura:if>

                                <!--  Drop Down  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Single Select~Drop Down',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <div class="">
                                        <div>
                                            <ui:outputText value="{!itemArr.Name}" />
                                        </div>
                                        <div>
                                            <select disabled="{!or(v.readOnly,isLocked)}" data-existing-value="{!itemArr.values}" data-old-id="{!question.check.Id}" id="{!question.check.Id +'-'+ attributeIndex}" data-Id="{!question.check.Id}" data-attributeIndex="{!attributeIndex}" value="{!itemArr.values==''?'None':itemArr.values}"
                                                data-index="{!index}" onchange="{!c.onOptionChange}" class="slds-select" style="padding:5px;padding-left:15px;padding-right:15px;width:100%;max-width: 100px;min-width: 100px;">
                                                <option>None</option>
                                                <aura:iteration items="{!itemArr.items}" var="option">
                                                    <option text="{!option.value}" value="{!option.value}"
                                                        selected="{!option.isSelected}"></option>
                                                </aura:iteration>
                                            </select>
                                        </div>
                                        <!-- <div style="margin-top:10px;">
                                            <aura:if
                                                isTrue="{!and(v.isCustomerCommunity,itemArr.buildertek__Image_Id__c)}">
                                                <img src="{!'/Customer/sfc/servlet.shepherd/version/download/'+itemArr.buildertek__Image_Id__c}"
                                                    width="150" height="150" data-id="{!index}"
                                                    data-checkId="{!question.check.Id}" />
                                            </aura:if>
                                            <aura:if
                                                isTrue="{!and(not(v.isCustomerCommunity),itemArr.buildertek__Image_Id__c)}">
                                                <img src="{!'/sfc/servlet.shepherd/version/download/'+ itemArr.buildertek__Image_Id__c}"
                                                    width="150" height="150" data-id="{!index}"
                                                    data-checkId="{!question.check.Id}" />
                                            </aura:if>
                                            <aura:if isTrue="{!!itemArr.buildertek__Image_Id__c}">
                                                <img src="{!$Resource.NoImageFound}" width="150" height="150"
                                                    data-id="{!index}" data-checkId="{!question.check.Id}" />
                                            </aura:if>
                                        </div> -->
                                    </div>
                                </aura:if>

                                <!--  Radio  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Single Select~Radio',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <div class="">
                                        <ui:outputText value="{!itemArr.Name}" />
                                        <div>
                                            <aura:iteration items="{!itemArr.items}" var="option">
                                                <input type="radio" disabled="{!or(v.readOnly,isLocked)}" name="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio'}" data-old-id="{!question.check.Id}" data-existing-value="{!itemArr.values}" id="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio'}"
                                                    data-Id="{!question.check.Id}" data-index="{!index}" onchange="{!c.onOptionChange}" checked="{!option.isSelected}" value="{!option.value}" />
                                                <label for="{!option.value}">{!option.value}</label><br />
                                            </aura:iteration>
                                        </div>
                                    </div>
                                </aura:if>

                                <!--  Radio-lineDirection  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Single Select~Radio-lineDirection',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <div class="">
                                        <ui:outputText value="{!itemArr.Name}" />
                                        <div>
                                            <aura:iteration items="{!itemArr.items}" var="option">
                                                <input type="radio" disabled="{!or(v.readOnly,isLocked)}" name="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio1'}" data-old-id="{!question.check.Id}" data-existing-value="{!itemArr.values}" id="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio1'}"
                                                    data-Id="{!question.check.Id}" data-index="{!index}" onchange="{!c.onOptionChange}" checked="{!option.isSelected}" value="{!option.value}" />
                                                <label for="{!option.value}">{!option.value}</label>
                                            </aura:iteration>
                                        </div>
                                    </div>
                                </aura:if>

                                <!--  Checkbox  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Multi Select~Checkbox',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <div>
                                        <ui:outputText value="{!itemArr.Name}" />
                                        <div>
                                            <aura:iteration items="{!itemArr.items}" var="option" indexVar="optionIndex">
                                                <input type="checkbox" disabled="{!v.readOnly}" name="{!option.Id}" id="{!option.Id}" onchange="{!c.onCheckBoxChange}" data-index="{!attributeIndex}" data-questionId="{!question.check.Id}" data-additionalCost="{!question.check.buildertek__Additional_Cost__c}"
                                                    checked="{!option.isSelected}" value="{!option.value}" />
                                                <label for="{!option.value}">{!option.value}</label><br />
                                            </aura:iteration>
                                        </div>
                                    </div>
                                </aura:if>

                                <!--  Checkbox-lineDirection  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Multi Select~Checkbox-lineDirection',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <div>
                                        <ui:outputText value="{!itemArr.Name}" />
                                        <div>
                                            <aura:iteration items="{!itemArr.items}" var="option" indexVar="optionIndex">
                                                <input type="checkbox" disabled="{!v.readOnly}" name="{!option.Id}" id="{!option.Id}" onchange="{!c.onCheckBoxChange}" data-index="{!attributeIndex}" data-questionId="{!question.check.Id}" data-additionalCost="{!question.check.buildertek__Additional_Cost__c}"
                                                    checked="{!option.isSelected}" value="{!option.value}" />
                                                <label for="{!option.value}">{!option.value}</label><br />
                                            </aura:iteration>
                                        </div>
                                    </div>
                                </aura:if>

                                <!--  Multi Select List  -->
                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Multi Select~Multi Select List',and(not(itemArr.isChild),itemArr.isParent))}">
                                    <ui:outputText value="{!itemArr.Name}" />
                                    <c:BT_MultiSelect options="{!itemArr.items}" additionalCost="{!question.check.buildertek__Additional_Cost__c}" values="{!itemArr.values}" class="avQuestionInput" width="100%" infoText="" />
                                </aura:if>

                                <!--  Date Picker'  -->
                                <aura:if isTrue="{!itemArr.Drop_Down_Value__c=='Date~Date Picker'}">
                                    <ui:outputText value="{!itemArr.Name}" />
                                    <lightning:input disabled="{!v.readOnly}" type="date" variant="label-hidden" required="{!question.check.buildertek__Mandatory__c}" value="{!itemArr.values}" />
                                </aura:if>
                            </aura:iteration>

                            <!--  Rich Table  -->
                            <aura:if isTrue="{!question.check.buildertek__Display_Layout__c == 'Rich Table'}">
                                Rich Table
                            </aura:if>
                        </td>

                        <td data-label="Child Attributes" style="width: 100%;white-space: normal;align-items: center;">
                            <aura:if isTrue="{!and(question.errorMessage != null,question.errorMessage != '')}">
                                <div style="float:left;margin-top: -6px;">
                                    <lightning:icon iconName="utility:warning" alternativeText="Error!" variant="error" title="{!question.errorMessage}" /> &nbsp;
                                </div>
                            </aura:if>
                            <table>
                                <thead></thead>
                                <tbody>
                                    <td style="border-top-color: transparent;">
                                        <aura:iteration items="{!question.responses}" var="itemArr" indexVar="attributeIndex">
                                            <td style="border-top-color: transparent;">
                                                <!--  Simple Text  -->
                                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Text~Simple Text',and(not(itemArr.isChild),not(itemArr.isParent)))}">
                                                    <div class="">
                                                        <ui:outputText value="{!itemArr.Name}" />
                                                        <lightning:input disabled="{!v.readOnly}" variant="label-hidden" class="" required="{!question.check.buildertek__Mandatory__c}" value="{!itemArr.values}" />
                                                    </div>
                                                </aura:if>

                                                <!--  Text Area  -->
                                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Text~Text Area',and(not(itemArr.isChild),not(itemArr.isParent)))}">
                                                    <div class="">
                                                        <ui:outputText value="{!itemArr.Name}" />
                                                        <lightning:textarea disabled="{!v.readOnly}" variant="label-hidden" class="" required="{!question.check.buildertek__Mandatory__c}" value="{!itemArr.values}" />
                                                    </div>
                                                </aura:if>

                                                <!--  Drop Down  -->
                                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Single Select~Drop Down',and(not(itemArr.isChild),not(itemArr.isParent)))}">
                                                    <div style="margin: 10px;">
                                                        <div>
                                                            <ui:outputText value="{!itemArr.Name}" />
                                                        </div>
                                                        <div>
                                                            <select disabled="{!or(v.readOnly,isLocked)}" data-existing-value="{!itemArr.values}" data-old-id="{!question.check.Id}" id="{!question.check.Id +'-'+ attributeIndex}" data-Id="{!question.check.Id}" data-attributeIndex="{!attributeIndex}" value="{!itemArr.values==''?'None':itemArr.values}"
                                                                data-index="{!index}" onchange="{!c.onOptionChange}" class="slds-select" style="padding:5px;padding-left:15px;padding-right:15px;width:100%;max-width: 100px;min-width: 100px;">
                                                                <option>None</option>
                                                                <aura:iteration items="{!itemArr.items}" var="option">
                                                                    <option text="{!option.value}"
                                                                        value="{!option.value}"
                                                                        selected="{!option.isSelected}"></option>
                                                                </aura:iteration>
                                                            </select>
                                                        </div>
                                                        <!-- <div style="margin-top:10px;">
                                                        <aura:if
                                                            isTrue="{!and(v.isCustomerCommunity,itemArr.buildertek__Image_Id__c)}">
                                                            <img src="{!'/Customer/sfc/servlet.shepherd/version/download/'+itemArr.buildertek__Image_Id__c}"
                                                                width="100" height="100" data-id="{!index}"
                                                                data-checkId="{!question.check.Id}" />
                                                        </aura:if>
                                                        <aura:if
                                                            isTrue="{!and(not(v.isCustomerCommunity),itemArr.buildertek__Image_Id__c)}">
                                                            <img src="{!'/sfc/servlet.shepherd/version/download/'+ itemArr.buildertek__Image_Id__c}"
                                                                width="100" height="100" data-id="{!index}"
                                                                data-checkId="{!question.check.Id}" />
                                                        </aura:if>
                                                        <aura:if isTrue="{!!itemArr.buildertek__Image_Id__c}">
                                                            <img src="{!$Resource.NoImageFound}" width="100" height="100"
                                                                data-id="{!index}" data-checkId="{!question.check.Id}" />
                                                        </aura:if>
                                                    </div> -->
                                                    </div>
                                                </aura:if>

                                                <!--  Radio  -->
                                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Single Select~Radio',and(not(itemArr.isChild),not(itemArr.isParent)))}">
                                                    <div class="">
                                                        <ui:outputText value="{!itemArr.Name}" />
                                                        <div>
                                                            <aura:iteration items="{!itemArr.items}" var="option">
                                                                <input type="radio" disabled="{!v.readOnly}" name="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio'}" data-old-id="{!question.check.Id}" data-existing-value="{!itemArr.values}" id="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio'}"
                                                                    data-Id="{!question.check.Id}" data-index="{!index}" onchange="{!c.onOptionChange}" checked="{!option.isSelected}" value="{!option.value}" />
                                                                <label for="{!option.value}">{!option.value}</label><br />
                                                            </aura:iteration>
                                                        </div>
                                                        <!--lightning:radioGroup disabled="{!v.readOnly}"  variant="label-hidden" name="{!itemArr.Id}" onchange="{!c.radioGroupOnchange}"
                                                                          options="{!itemArr.items}"
                                                                          value="{!itemArr.values}"
                                                                          type="radio" required="{!question.check.buildertek__Mandatory__c}"
                                                                          class="avQuestionInput radio"
                                                                          messageWhenValueMissing="Please select option"/-->
                                                    </div>
                                                </aura:if>

                                                <!--  Radio-lineDirection  -->
                                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Single Select~Radio-lineDirection',and(not(itemArr.isChild),not(itemArr.isParent)))}">
                                                    <div class="">
                                                        <ui:outputText value="{!itemArr.Name}" />
                                                        <div>
                                                            <aura:iteration items="{!itemArr.items}" var="option">
                                                                <input type="radio" disabled="{!v.readOnly}" name="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio1'}" data-old-id="{!question.check.Id}" data-existing-value="{!itemArr.values}" id="{!question.check.Id +'-'+ attributeIndex+'-'+option.value+'radio1'}"
                                                                    data-Id="{!question.check.Id}" data-index="{!index}" onchange="{!c.onOptionChange}" checked="{!option.isSelected}" value="{!option.value}" />
                                                                <label for="{!option.value}">{!option.value}</label>
                                                            </aura:iteration>
                                                        </div>

                                                        <!--lightning:radioGroup variant="" name="{!question.check.Id}"
                                                                          options="{!itemArr.items}"
                                                                          value="{!itemArr.values}"
                                                                          type="radio" 
                                                                          required="{!question.check.buildertek__Mandatory__c}" 
                                                                          class="avQuestionInput horizontal_radio radio"
                                                                          disabled="{!v.readOnly}"  /-->
                                                    </div>
                                                </aura:if>

                                                <!--  Checkbox  -->
