                                                        <div>
                                                            <aura:iteration items="{!itemArr.items}" var="option" indexVar="optionIndex">
                                                                <input type="checkbox" disabled="{!v.readOnly}" name="{!option.Id}" id="{!option.Id}" onchange="{!c.onCheckBoxChange}" data-index="{!attributeIndex}" data-questionId="{!question.check.Id}" data-additionalCost="{!question.check.buildertek__Additional_Cost__c}"
                                                                    checked="{!option.isSelected}" value="{!option.value}" />
                                                                <label for="{!option.value}">{!option.value}</label><br />
                                                            </aura:iteration>
                                                        </div>
                                                    </div>

                                                </aura:if>

                                                <!--  Multi Select List  -->
                                                <aura:if isTrue="{!and(itemArr.Drop_Down_Value__c=='Multi Select~Multi Select List',and(not(itemArr.isChild),not(itemArr.isParent)))}">
                                                    <ui:outputText value="{!itemArr.Name}" />
                                                    <c:BT_MultiSelect options="{!itemArr.items}" additionalCost="{!question.check.buildertek__Additional_Cost__c}" values="{!itemArr.values}" class="avQuestionInput" width="100%" infoText="" />
                                                </aura:if>
                                            </td>
                                        </aura:iteration>
                                    </td>
                                </tbody>
                            </table>


                            <!--  Rich Table  -->
                            <aura:if isTrue="{!question.check.buildertek__Display_Layout__c == 'Rich Table'}">
                                Rich Table
                            </aura:if>
                        </td>

                        <!-- Actions -->
                        <aura:if isTrue="{!!v.isResponse}">
                            <td data-label="Actions" style="width: 2%;white-space: normal">
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col">
                                        <div class="{!if(v.callerName == 'builder','accordion_edit','slds-hide')}" data-selected-index="{!index}">
                                            <a href="javascript:void(0);" onclick="{!c.editQuestion}" data-selected-index="{!index}">
                                                <lightning:icon iconName="utility:edit" size="x-small" />
                                            </a>&nbsp;
                                        </div>
                                    </div>
                                </div>
                                <div class="slds-grid slds-gutters">
                                    <div class="slds-col">
                                        <div class="{!if(v.callerName == 'builder','accordion_edit','slds-hide')}" data-selected-index="{!index}">
                                            <a href="javascript:void(0);" onclick="{!c.deleteRecordModal}" data-selected-index="{!index}">
                                                <lightning:icon iconName="utility:delete" size="x-small" />
                                            </a>&nbsp;
                                        </div>
                                    </div>
                                </div>
                            </td>
                            <aura:set attribute="else">
                                <td data-label="Actions" style="width: 2%;white-space: normal">
                                    <div class="slds-grid slds-gutters" style="float:right;">
                                        <div class="slds-col">
                                            <div class="{!if(v.callerName == 'builder','accordion_edit','slds-show')}" data-selected-index="{!index}" style="float:rigth;">
                                                <a href="javascript:void(0);" onclick="{!c.viewDetails}" data-selected-index="{!index}">
                                                    <lightning:icon iconName="utility:edit" size="x-small" />
                                                </a>&nbsp;
                                            </div>
                                        </div>
                                    </div>
                                </td>
                            </aura:set>
                        </aura:if>
                    </tr>
                </aura:if>
            </aura:iteration>
        </tbody>
    </table>

    <lightning:overlayLibrary aura:id="overlayLib" />
    <aura:if isTrue="{!v.isOptionImageDelete}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                <header class="slds-modal__header">
                    <h1>Delete Confirmation</h1>
                </header>
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    Are you sure you want to delete this option Image?
                </div>
                <footer class="slds-modal__footer">
                    <lightning:button variant="neutral" label="NO" title="No" onclick="{! c.closeDeleteImage }" />
                    <lightning:button variant="brand" label="Yes" title="Yes" onclick="{! c.deleteConfirmImage }" />
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>

    <aura:if isTrue="{!v.hasModalOpen}">
        <section onclick="{!c.closeModel}" role="dialog" aria-modal="true" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container sldsmodalcontainer">
                <div class="slds-modal__content slds-p-around_medium slds-text-align_center" style="background: transparent;">
                    <div style="width: 55%; margin: 0 auto; text-align: left">
                        <lightning:fileCard fileId="{!v.currentDocumentId}" />
                    </div>
                </div>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
</aura:component>