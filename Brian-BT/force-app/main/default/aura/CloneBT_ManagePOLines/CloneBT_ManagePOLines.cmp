                                                                    <c:BT_FieldSetMassUpdate record="{!row}" index="{!index}" field="{!field}"
                                                                                             isMassUpdateEnable="{!v.massUpdateEnable}"
                                                                                             childObjectName="buildertek__Select_Sheet__c" 
                                                                                             ObjectName="buildertek__Select_Sheet__c"/>
                                                                </td>
                                                                <aura:set attribute="else">
                                                                    <td>  
                                                                        <c:BT_FieldSetMassUpdate record="{!row}" index="{!index}" field="{!field}"
