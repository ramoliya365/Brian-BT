                                                                                     childObjectName="buildertek__Select_Sheet__c"
                                                                                     ObjectName="buildertek__Select_Sheet__c"/>
                                                            
                                                        </td> 
                                                        <aura:set attribute="else"> 
                                                            <aura:if isTrue="{! or( or(field.type == 'DOUBLE',field.type == 'CURRENCY'), field.type == 'PERCENT') }">
                                                                
